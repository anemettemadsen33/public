# API Configuration
VITE_API_URL=http://localhost:3000/api

# OpenAI Configuration (for AI Chatbot)
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4o-mini
VITE_USE_REAL_AI=false

# VIN Check API Configuration
# You can use NHTSA (free) or commercial services like Carfax, AutoCheck
VITE_VIN_API_KEY=your_vin_api_key_here
VITE_VIN_API_URL=https://api.vindecoder.eu/3.2

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_APPLE_CLIENT_ID=your_apple_client_id

# Email Service Configuration
VITE_EMAIL_FROM=noreply@automarketplace.com

# Google Maps API (for Interactive Map)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Push Notifications (Web Push)
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key

# Feature Flags
VITE_ENABLE_2FA=true
VITE_ENABLE_MAGIC_LINK=true
VITE_ENABLE_REAL_VIN_CHECK=false
VITE_ENABLE_EMAIL_NOTIFICATIONS=true
